{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "allowJs": false,
    "resolveJsonModule": true,
    "noEmit": true,
    "checkJs": false,
    "jsx": "react",
    "declaration": false,
    "outDir": "./dist",
    "rootDir": "./",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "sourceMap": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*", "src/*", "node_modules/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
